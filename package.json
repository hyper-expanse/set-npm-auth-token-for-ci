    "automation",
    "npmrc",
    "npm",
    "ci",
    "token"
  ],
  "license": "Apache-2.0",
  "main": "src/index.js",
  "name": "@hutson/set-npm-auth-token-for-ci",
  "renovate": {
    "extends": [
      "config:base",
      ":maintainLockFilesWeekly",
      ":rebaseStalePrs",
