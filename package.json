    "nyc": "^13.0.0",
    "semantic-release-gitlab": "^7.0.0",
    "semistandard": "^13.0.1",
    "sinon": "^7.0.0",
    "sinon-chai": "^3.2.0",
    "tmp": "^0.0.33"
  }
